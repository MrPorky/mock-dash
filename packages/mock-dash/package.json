{
  "name": "mock-dash",
  "type": "module",
  "version": "0.2.9",
  "description": "A TypeScript library for generating type-safe API clients and mock servers from Zod schemas",
  "author": "Eskil Ganslandt",
  "license": "MIT",
  "homepage": "https://github.com/MrPorky/mock-dash",
  "bugs": "https://github.com/MrPorky/mock-dash/issues",
  "keywords": [
    "typescript",
    "zod",
    "api",
    "client",
    "mock",
    "server",
    "type-safe",
    "schema",
    "validation",
    "hono"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "files": [
    "dist"
  ],
  "bin": {
    "mock-dash": "dist/cli.js"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@hono/node-ws": "^1.2.0",
    "zod": "^4.0.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@hono/node-ws": "^1.2.0",
    "openapi-types": "^12.1.3",
    "tsdown": "^0.16.5"
  }
}
