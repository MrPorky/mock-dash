{
  "name": "@examples/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared models, schemas, and server code for mock-dash examples",
  "main": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./src/server/index.ts"
    }
  },
  "scripts": {
    "gen:schemas": "pnpm exec mock-dash generate ./src/swagger.json -o ./src/schemas/auth.gen.ts -prbd",
    "build": "tsc",
    "check": "tsc --noEmit",
    "dev": "tsx src/server/index.ts"
  },
  "dependencies": {
    "hono": "^4.11.7",
    "mock-dash": "workspace:*"
  },
  "devDependencies": {
    "tsx": "^4.21.0"
  },
  "files": [
    "src",
    "dist"
  ]
}
