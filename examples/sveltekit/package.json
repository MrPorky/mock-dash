{
  "name": "sveltekit",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "pnpm --filter @examples/shared dev",
    "dev:client": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "prepare": "svelte-kit sync || echo ''"
  },
  "dependencies": {
    "@examples/shared": "workspace:*",
    "mock-dash": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "svelte": "^5.43.6",
    "svelte-check": "^4.3.3"
  }
}
